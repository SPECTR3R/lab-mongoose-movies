

<h1>register a new celebrity</h1>

<form action="/celebrities" method="post">
  <label>Name</label>
  <input type="text" name="name" id="name">
  <label>Occupation</label>
  <input type="text" name="occupation" id="occupation">
  <label>Catch Phrase</label>
  <input type="text" name="catchPhrase" id="catchPhrase">
  <input type="submit" value="Add Celebrity">
</form>

{{#if error}}
<p>already in DB</p>
{{/if}}
<a href="/celebrities">Go Back</a>
